{
  "name": "GitHub Open in Tower",
  "description": "This extension allows you to open the current repository and clone it with Tower.",
  "version": "1.0.0",
  "manifest_version": 3,
  "author": "aPinix",
  "action": {
    "default_title": "GitHub Open in Tower",
    "default_icon": {
      "16": "./images/icons/icon-16.png",
      "32": "./images/icons/icon-32.png",
      "48": "./images/icons/icon-48.png",
      "128": "./images/icons/icon-128.png"
    }
  },
  "icons": {
    "16": "./images/icons/icon-16.png",
    "32": "./images/icons/icon-32.png",
    "48": "./images/icons/icon-48.png",
    "128": "./images/icons/icon-128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "js": ["contentscript.js"],
      "matches": ["https://github.com/*"]
    }
  ]
}
